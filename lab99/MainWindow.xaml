<Window x:Class="lab99.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab99"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="header">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Snippet name" Binding="{Binding snippetname}"/>
                <DataGridTextColumn Header="snippet type" Binding="{Binding snippetype}"/>
                <DataGridTextColumn Header="Update Time" Binding="{Binding UpdateTime}"/>
            </DataGrid.Columns>
        </DataGrid> 
    </Grid>
</Window>
